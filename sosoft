function GetWorld()
    local _world = game.PlaceId
    if _world == 7449423635 then return 3 end 
    if _world == 4442272183 then return 2 end 
    if _world == 2753915549 then return 1 end 
end
local world = GetWorld() 
if world ~= 3 then
_G.Fruits_Settings = {
    ['Main_Fruits'] = {'Dough-Dough','Sound-Sound'},
    ['Select_Fruits'] = {"Buddha-Buddha", "Dark-Dark", "Light-Light", "Ice-Ice", "Magma-Magma"} 
}
getgenv().Key = "MARU-GCDC-DFBJ3-6HYV-RZ4FW-A4YA"
getgenv().id = "302097681378181121"
getgenv().Script_Mode = "Kaitun_Script"
loadstring(game:HttpGet("https://raw.githubusercontent.com/xshiba/MaruBitkub/main/Mobile.lua"))()
else 
_G.Delay_Upload = 300 -- โชว์ข้อมูลใหม่ทุกๆ 300วิ
_G.Bypass_Tp = true -- วาปฟาร์ม
