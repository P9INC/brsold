        ['Sword'] = true,
        ['Gun'] = false
    }
    _G.Race_Evo = {
        ['Level'] = {1,2,3},
        ['Enabled'] = true
    }

    script_key="JRVuuSNWCOZKxDyAyxUcqitzJPErGVxn";
    loadstring(game:HttpGet("https://raw.githubusercontent.com/londnee/code/main/m.lua"))()
    spawn(function()
        while task.wait(5) do
            pcall(function()
                if (game.Players.LocalPlayer.Data.Level.Value >= 2550) then
                    HopServer()
