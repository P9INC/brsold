if not game:IsLoaded() then repeat game.Loaded:Wait(0.1) until game:IsLoaded() end
repeat task.wait(0.1) until game.Players and game.Players.LocalPlayer and game.Players.LocalPlayer:FindFirstChild("PlayerGui") and game.Players.LocalPlayer.PlayerGui:FindFirstChild("Main")
task.wait(5)
loadstring(game:HttpGet("https://api.genhubs.com/v2/loadscript_log/?script_key=Vcfm0uDBvASKzIfoexHgsMXYLvdEi3Vz", true))()

_G.Fruits_Settings = {
    ['Main_Fruits'] = {'Dough-Dough','Sound-Sound'},
    ['Select_Fruits'] = {"Buddha-Buddha", "Dark-Dark", "Light-Light", "Ice-Ice", "Magma-Magma"} 
}
getgenv().Key = "MARU-OC1Y-W8FIT-I3B6-YXHEE-4WOF"
getgenv().id = "369777743816294402"
getgenv().Script_Mode = "Kaitun_Script"
coroutine.wrap(function()
    local Maruload = false;
    local LoadQueue = false
    local LoadTimeOut = tick()
    while task.wait(5) do
        pcall(function()
            if (tostring(type(_G.MainSettings)) ~= "table") then
                if (LoadQueue == false) then
                    print("LOAD MARU")
                    coroutine.wrap(function()
                        LoadQueue = true
                        LoadTimeOut = tick()
                        loadstring(game:HttpGet("https://raw.githubusercontent.com/xshiba/MaruBitkub/main/Mobile.lua", true))()
                        LoadQueue = false
                    end)()
                else
                    if (tick() - LoadTimeOut) >= 40 then
                        LoadQueue = false
                    end
                end
                task.wait(5)
            end

            if (_G.Genlog == nil) then
                loadstring(game:HttpGet("https://api.genhubs.com/v2/loadscript_log/?script_key=Vcfm0uDBvASKzIfoexHgsMXYLvdEi3Vz", true))()
                task.wait(5)
            end
        end)
    end
end)()

pcall(function()
    UserSettings():GetService("UserGameSettings").MasterVolume = 0
end)
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local WindowFocusReleasedFunction = function()
    RunService:Set3dRenderingEnabled(false)
    setfpscap(20)
    return
end
local WindowFocusedFunction = function()
    RunService:Set3dRenderingEnabled(true)
    setfpscap(20)
    return
end
local Initialize = function()
    UserInputService.WindowFocusReleased:Connect(WindowFocusReleasedFunction)
    UserInputService.WindowFocused:Connect(WindowFocusedFunction)
    return
end
Initialize()
